<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:background="#ffffff"
	tools:context=".IndexActivity" >
	<ImageView
		android:id="@+id/view_logo"
		android:layout_width="159dp"
		android:layout_height="60dp"
		android:layout_alignParentTop="true"
		android:layout_centerHorizontal="true"
		android:clickable="false"
		android:contentDescription="@string/logo_"
		android:longClickable="false"
		android:maxHeight="60dp"
		android:maxWidth="159dp"
		android:src="@drawable/logo_ix" />
	<TextView
		android:id="@+id/label_nominativo"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentLeft="true"
		android:layout_below="@+id/view_logo"
		android:gravity="left"
		android:hint="@string/nomin_interv"
		android:paddingLeft="4dp"
		android:textIsSelectable="false" />
	<ListView
		android:id="@+id/list_interv_assigned"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_below="@id/label_nominativo"
		android:layout_marginTop="4dp" >
	</ListView>
</RelativeLayout>